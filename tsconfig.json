{
    "compilerOptions": {
        "target": "ES2017",
        "module": "commonjs",
        "strict": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": [
          "./node_modules/@types",
          "./types" 
            
        ],
        "types": ["node", "express"] 
    },
    "include": ["./**/*", "routes/webRTC/.ts"],
    "exclude": ["node_modules", "dist"]
  }
  